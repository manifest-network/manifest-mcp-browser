{
  "name": "@manifest-network/manifest-mcp-browser",
  "version": "0.1.11",
  "description": "Browser-compatible MCP server for Cosmos SDK blockchain interactions with Manifest Network using CosmJS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc --watch",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "docs": "typedoc",
    "version": "node -e \"var v=require('./package.json').version;require('fs').writeFileSync('src/version.ts','export const VERSION = \\x27'+v+'\\x27;\\n')\" && git add src/version.ts"
  },
  "keywords": [
    "mcp",
    "cosmos",
    "cosmjs",
    "blockchain",
    "manifest",
    "browser"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/manifest-network/manifest-mcp-browser"
  },
  "author": "Felix C. Morency",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@cosmjs/encoding": "0.32.4",
    "@cosmjs/proto-signing": "0.32.4",
    "@cosmjs/stargate": "npm:@manifest-network/stargate@0.32.4-ll.3",
    "@manifest-network/manifestjs": "2.2.0-billing-v2.1",
    "@modelcontextprotocol/sdk": "1.25.3",
    "limiter": "3.0.0"
  },
  "overrides": {
    "@cosmjs/stargate": "npm:@manifest-network/stargate@0.32.4-ll.3"
  },
  "devDependencies": {
    "@types/node": "20.19.30",
    "typedoc": "^0.28.16",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  }
}
